{
    "compilerOptions": {
        "target": "ES2021",
        "module": "CommonJS",
        "outDir": "dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "moduleResolution": "Node",
        "declaration": true
    }
}